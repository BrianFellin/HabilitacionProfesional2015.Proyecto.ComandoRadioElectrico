<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ComandoRadioElectrico.Core.Model" assembly="ComandoRadioElectrico.Core">
  <class name="Balance" table="Balance" lazy="false">
    <id name="Id" type="int" column="Id">
      <generator class="identity"/>
    </id>
    <property name="DateFrom" column="FechaDesde"/>
    <property name="DateUntil" column="FechaHasta"/>    
    <property name="DateOfRealization" column="FechaRealizacion"/>
    <property name="Description" column="Descripcion"/>
  </class>
</hibernate-mapping>
