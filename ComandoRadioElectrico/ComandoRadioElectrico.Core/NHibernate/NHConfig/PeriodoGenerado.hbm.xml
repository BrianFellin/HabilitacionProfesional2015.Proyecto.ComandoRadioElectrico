<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ComandoRadioElectrico.Core.NHibernate.Model" assembly="ComandoRadioElectrico.Core">
  <class name="GeneratedPeriod" table="PeriodoGenerado" lazy="false">
    <id name="Id" type="int" column="Id">
      <generator class="identity"/>
    </id>    
    <property name="Month" column="Mes"/>    
    <property name="Year" column="Año"/>
        
  </class>
</hibernate-mapping>
