<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ComandoRadioElectrico.Core.NHibernate.Model" assembly="ComandoRadioElectrico.Core">
  <class name="Person" table="Persona">
    <id name="Id" type="int" column="Id">
      <generator class="identity"/>
    </id>
    <property name="DateFrom" column="FechaDesda"/>
    <property name="DateUntil" column="FechaHasta"/>    
    <property name="DateOfRealization" column="FechaRealizacion"/>
    <property name="Description" column="Descripcion"/>

    <many-to-one name="Partner"
                 column="IdSocio" class="Partner"
                 not-null="true"/>
  </class>
</hibernate-mapping>
